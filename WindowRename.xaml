<Window x:Name="windowRename" x:Class="SimpleTagManager.WindowRename"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleTagManager"
        mc:Ignorable="d"
        Title="Rename" Height="139.234" Width="377.512">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        
        
        <TextBox x:Name="textBox" HorizontalAlignment="Stretch" Height="25" Margin="20" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Padding="3"/>
        <Button x:Name="buttonOK" Content="OK" HorizontalAlignment="right" VerticalAlignment="Top" Width="75" Margin="20, 0, 115, 20" Grid.Row="1" Height="25" Click="buttonOK_Click"/>
        <Button x:Name="buttonCancel" Content="Cancel" HorizontalAlignment="right" VerticalAlignment="Top" Width="75" Margin="20, 0, 20, 20" Grid.Row="1" Height="25" Click="buttonCancel_Click"/>

    </Grid>
</Window>
